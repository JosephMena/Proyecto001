    <mapping resource="capuli/dominino/TipoMenu.hbm.xml"/>
    <mapping resource="capuli/dominino/Bebida.hbm.xml"/>
    <mapping resource="capuli/dominino/Postre.hbm.xml"/>
    <mapping resource="capuli/dominino/Pedido.hbm.xml"/>
    <mapping resource="capuli/dominino/Segundo.hbm.xml"/>
    <mapping resource="capuli/dominino/Entrada.hbm.xml"/>
    <mapping resource="capuli/dominino/Comensal.hbm.xml"/>
    <mapping resource="capuli/dominino/Menu.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
